   <record id="udos_set_form" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <form>
                <sheet string="Sales Sets">
                    <group name="set_description">
                        <field name="name" placeholder="Referencia para seleccionar al crear presupuestos"/>
                        <field name="set_name" placeholder="Nombre de la sección o apartado"/>
                        <field name="set_description" placeholder="Contenido ampliado que se imprimirá bajo el nombre de sección"/>
                    </group>
                    <group name="set_clasificaciones">
                        <group>
                            <field name="department_id"/>
                            <field name="sector_id"/>
                            <field name="quality_id"/>
                        </group>
                        <group>
                            <field name="brand_id"/>
                            <field name="serie_id"/>
                            <field name="type_id"/>
                        </group>
                    </group>
                    <group col="2">
                        <field name="line_ids" colspan="4" can_create="true" can_write="true" modifiers="{}"/>
                    </group>
                </sheet>
            </form>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">set</field>
        <field name="name">Set Form</field>
        <field name="priority">16</field>
        <field name="type">form</field>
    </record>

    <record id="udos_set_tree" model="ir.ui.view">
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <tree string="Sets">
                <field name="name" modifiers="{}"/>
                <field name="set_name"/>
                <field name="department_id"/>
                <field name="sector_id"/>
                <field name="quality_id"/>
                <field name="brand_id"/>
                <field name="serie_id"/>
                <field name="type_id"/>
            </tree>
        </field>
        <field name="field_parent" eval="False"/>
        <field name="groups_id" eval="[(6, 0, [])]"/>
        <field name="inherit_id" eval="False"/>
        <field name="key" eval="False"/>
        <field name="mode">primary</field>
        <field name="model">set</field>
        <field name="name">Set Tree</field>
        <field name="priority">16</field>
